{
  "singleQuote": true,
  "printWidth": 80,
  "trailingComma": "all",
  "semi": false,
  "jsxSingleQuote": true,
  "bracketSpacing": true,
  "bracketSameLine": false,
  "singleAttributePerLine": false,
  "arrowParens": "always",
  "tabWidth": 2,
  "plugins": [
    "prettier-plugin-organize-imports"
  ]
}
